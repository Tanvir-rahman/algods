{
  "name": "js-algo-ds",
  "version": "0.0.1",
  "description": "Data structure and algorithm problems in a library",
  "main": "./js-algo-ds.cjs.js",
  "module": "./js-algo-ds.esm.js",
  "browser": "./js-algo-ds.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --fix",
    "test": "jest --colors --notify",
    "test:watch": "jest --watch",
    "test:coverage": "yarn run test -- --coverage",
    "test:badges": "yarn run test:coverage  && jest-coverage-badges",
    "publish": "yarn run lint && yarn run test && yarn run build && yarn run test:badges && yarn publish"
  },
  "keywords": [
    "js-algo-ds",
    "jsalgods",
    "algods",
    "algo-ds",
    "dsalgo",
    "ds-algo",
    "algodsjs",
    "algorithms",
    "data structures",
    "javascript",
    "js",
    "problem solving",
    "problem solver",
    "leetcode",
    "npm",
    "yarn"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tanvir-rahman/js-algo-ds.git"
  },
  "bugs": {
    "url": "https://github.com/Tanvir-rahman/js-algo-ds/issues"
  },
  "homepage": "https://github.com/Tanvir-rahman/js-algo-ds#readme",
  "author": "Tanvir Rahman <rahmantanvir25558@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "jest": "26.5.3",
    "jest-coverage-badges": "^1.1.2",
    "rollup": "2.32.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "7.0.2"
  }
}
